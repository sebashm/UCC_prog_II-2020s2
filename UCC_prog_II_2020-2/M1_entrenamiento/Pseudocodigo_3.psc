Proceso Pseudocodigo_3
	//Definir variables
	Definir a, b Como Entero; 
	Definir s Como Logico; 
	
	a <- 1;
	b <- 0; 
	
	Escribir "¿Napoleon nació en Francia? (0) Falso, (1) Verdadero: ", a; 
	Leer a; 
	
	Escribir "¿La Torre Eiffel está en Australia? (0) Falso, (1) Verdadero: ", b;
	Leer b; 
	
	s <- a > b; 
	
	Escribir "El valor final de la operación es: ", s; 
	
FinProceso
